import java.util.HashMap;
import java.util.List;
import java.util.Map;

public class Etl {
    
    public static Map<Integer, List<String>> transform(Map<Integer, List<String>> old) {
        
        Integer values[] = {1,2,3,4,5,8,10};
        
        Map<Integer,List<String>> result = new HashMap<>();
        
        for(Integer i:values){
            if(old.containsKey(values[i])) {
            result.put(values[i], old.get(values[i]));
        }
      }    
        
        return result;
    }
}
